	@rm -f $(OUT)/HOL-Plain $(OUT)/HOL-Main $(OUT)/HOL		\
		$(OUT)/HOL-Nominal $(OUT)/TLA $(LOG)/HOL.gz		\
		$(LOG)/TLA.gz $(LOG)/HOL-Isar_Examples.gz		\
		$(LOG)/HOL-Induct.gz $(LOG)/HOL-ex.gz			\
